{
  "name": "backend",
  "version": "1.0.0",
  "description": "**Edit a file, create a new file, and clone from Bitbucket in under 2 minutes**",
  "main": "./src/index.ts",
  "scripts": {
    "build": "npx tsc --outDir ./dist",
    "start": "node ./dist/index.js",
    "dev": "concurrently \"npx tsc --watch\" \"nodemon -q ./dist/index.js\"",
    "lint:fix": "TIMING=1 eslint .src/ --fix",
    "lint": "TIMING=1 eslint .src/ --max-warnings 0"
  },
  "repository": {
    "type": "git",
    "url": "git@personal.bitbucket.org:bigdict-tg/backend.git"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^16.3.1",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^20.5.2",
    "concurrently": "^8.2.1",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "nodemon": "^3.0.1"
  }
}
